{
  "workspace": "~/workspace",
  "gateway": {
    "port": 48391,
    "name": "orchestrator",
    "role": "coordinator",
    "model": "claude-opus-4"
  },
  "agents": [
    {
      "name": "frontend-lead",
      "port": 48420,
      "role": "frontend-team-lead",
      "model": "claude-sonnet-4",
      "token": "emp-fe-lead-token"
    },
    {
      "name": "frontend-dev-1",
      "port": 48440,
      "role": "frontend-developer",
      "model": "codex",
      "token": "emp-fe-dev1-token"
    },
    {
      "name": "frontend-dev-2",
      "port": 48460,
      "role": "frontend-developer",
      "model": "codex",
      "token": "emp-fe-dev2-token"
    },
    {
      "name": "backend-lead",
      "port": 48520,
      "role": "backend-team-lead",
      "model": "claude-sonnet-4",
      "token": "emp-be-lead-token"
    },
    {
      "name": "backend-dev-1",
      "port": 48540,
      "role": "backend-developer",
      "model": "codex",
      "token": "emp-be-dev1-token"
    },
    {
      "name": "qa-lead",
      "port": 48620,
      "role": "qa-manager",
      "model": "claude-sonnet-4",
      "token": "emp-qa-lead-token"
    },
    {
      "name": "qa-tester",
      "port": 48640,
      "role": "qa-tester",
      "model": "codex",
      "token": "emp-qa-tester-token"
    }
  ],
  "endpoints": [
    { "name": "staging", "url": "https://staging.myproduct.com" },
    { "name": "production", "url": "https://myproduct.com" },
    { "name": "api-staging", "url": "https://api-staging.myproduct.com/health" },
    { "name": "api-production", "url": "https://api.myproduct.com/health" }
  ],
  "repos": [
    { "name": "frontend", "repo": "myorg/frontend" },
    { "name": "backend", "repo": "myorg/backend" },
    { "name": "shared-libs", "repo": "myorg/shared-libs" },
    { "name": "infra", "repo": "myorg/infrastructure" }
  ],
  "services": [
    "openclaw-gateway",
    "openclaw-gateway-frontend-lead",
    "openclaw-gateway-backend-lead",
    "openclaw-gateway-qa-lead"
  ],
  "linear": {
    "teams": ["FE", "BE", "QA"],
    "apiKeyEnv": "LINEAR_API_KEY"
  }
}
